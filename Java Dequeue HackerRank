    import java.util.*;
    public class test {
        public static void main(String[] args) {
            Scanner in = new Scanner(System.in);
            Deque deque = new ArrayDeque<>();
            int n = in.nextInt();
            int m = in.nextInt();
            for (int i = 0; i < m; i++) {
                int num = in.nextInt();
                deque.add(num);
            }
            int un = find(deque);
            for (int i = m; i < n; i++) {
                int num = in.nextInt();
                deque.remove();
                deque.add(num);
                int un1=find(deque);
                if(un1>un){
                    un=un1;
                }
            }
            System.out.print(un);
        }
        public static int find(Deque deque){
            Set<Integer> set = new HashSet<Integer>();
            set.addAll(deque);
            return set.size();
        }
    }



