import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        for(int i =1;in.hasNext()== true;i++){
            System.out.println(i+" "+in.nextLine());
        }
    }
}


